<?php
/**
 * @file
 * Configuration refresh module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function config_refresh_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'config_refresh.overview':
      return '<p>' . t('Allows to refresh the configuration provided by a module/theme/profile with current system changes.') . '</p>';
    break;
  }
}
